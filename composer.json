{
    "name": "Tessi-Tms/JobBundle",
    "type": "symfony-bundle",
    "description": "Bundle providing a cron-powered task dispatcher.",
    "keywords": ["bundle", "scheduler", "job", "task"],
    "homepage": "https://github.com/Tessi-Tms/JobBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Tessi TMS",
            "email": "contact@tessi.fr"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/http-foundation": ">=2.1,<2.4",
        "symfony/framework-bundle": ">=2.1,<2.4",
        "symfony/security-bundle": ">=2.1,<2.4",
        "symfony/twig-bundle": ">=2.1,<2.4",
        "symfony/finder": ">=2.1,<2.4",
        "vvg/filterlist-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Tessi\\JobBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Tessi/JobBundle"
}
